#!/usr/bin/env python3
import sys
import os

# Add parent directory to python path to find computechain module
script_dir = os.path.dirname(os.path.abspath(__file__))
parent_dir = os.path.dirname(script_dir)
sys.path.insert(0, parent_dir)

from computechain.cli.main import main

if __name__ == "__main__":
    main()

